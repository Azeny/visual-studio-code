{
  "name": "azeny",
  "displayName": "Azeny Theme",
  "description": "A Night Theme for Night Programmers",
  "version": "0.1.0",
  "publisher": "Azeny",
  "license": "MIT",
  "icon": "assets/logo.png",
  "scripts": {
    "publish": "vsce publish"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "dark",
    "theme",
    "azeny",
    "color-theme"
  ],
  "galleryBanner": {
    "color": "#1c1d28",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Azeny",
        "uiTheme": "vs-dark",
        "path": "./themes/azeny.json"
      },
      {
        "label": "Azeny Inveny",
        "uiTheme": "vs-dark",
        "path": "./themes/inveny.json"
      },
      {
        "label": "Azeny Okano",
        "uiTheme": "vs-dark",
        "path": "./themes/okano.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azeny/vscode-azeny.git"
  },
  "bugs": {
    "url": "https://github.com/Azeny/vscode-azeny/issues"
  },
  "homepage": "https://github.com/Azeny/vscode-azeny#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
