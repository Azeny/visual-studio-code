{
  "name": "azeny",
  "displayName": "Azeny Theme",
  "description": "A Night Theme for Night Programmers",
  "version": "0.0.16",
  "publisher": "Azeny",
  "license": "MIT",
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "dark",
    "theme",
    "color-theme"
  ],
  "galleryBanner": {
    "color": "#1c1d28",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "Azeny",
        "uiTheme": "vs-dark",
        "path": "./themes/azeny.json"
      },
      {
        "label": "Azeny Invert",
        "uiTheme": "vs-dark",
        "path": "./themes/azeny-secondary.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Azeny/visual-studio-code.git"
  },
  "bugs": {
    "url": "https://github.com/Azeny/visual-studio-code/issues"
  },
  "homepage": "https://github.com/Azeny/visual-studio-code#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "husky": "^4.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
